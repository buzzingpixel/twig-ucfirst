{
    "name": "buzzingpixel/twig-ucfirst",
    "version": "1.0.0",
    "description": "A Twig ucfirst filter",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "twig/twig": "^2.5"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "buzzingpixel\\twigucfirst\\": "src"
        }
    },
    "extra": {
        "twigExtensions": [
            "buzzingpixel\\twigucfirst\\UcFirstTwigExtension"
        ]
    }
}
